<h1> <%= @review.title %> </h1>
<h3> <%= @review.movie.title %> </h3>
<h4> By <%= @review.user_id %></h4>

<p> <%= @review.content %> </p>

<p> What did you think of this? </p>
<%= render 'rating' %>

<p> <% if @review.photo_1.file? %>
    <%= image_tag @review.photo_1.url(:medium) %>
  <% end %>
<% if @review.photo_2.file? %>
  <%= image_tag @review.photo_2.url(:medium) %>
<% end %>
<% if @review.photo_3.file? %>
  <%= image_tag @review.photo_3.url(:medium) %>
<% end %>
<% if @review.photo_4.file? %>
  <%= image_tag @review.photo_4.url(:medium) %>
<% end %>
<% if @review.photo_5.file? %>
  <%= image_tag @review.photo_5.url(:medium) %>
<% end %></p>
